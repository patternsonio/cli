{
  "name": "@patternson/cli",
  "version": "1.3.1",
  "description": "command line interface for patternson",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "concurrently -n 'babel,stencil' \"babel src -d lib\" \"cd libraryView && stencil build\"",
    "build:watch": "concurrently -n 'babel,stencil' \"babel src -d lib --watch\" \"cd libraryView && stencil build --watch\"",
    "prepare": "npm run build",
    "lint": "eslint ."
  },
  "bin": {
    "patternson": "lib/cli/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/patternsonio/cli.git"
  },
  "keywords": [
    "cli",
    "patternson",
    "pattern",
    "library",
    "hosting"
  ],
  "author": "Hannes Diercks <patternson-cli@xiphe.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patternsonio/cli/issues"
  },
  "homepage": "https://github.com/patternsonio/cli#readme",
  "devDependencies": {
    "@types/node": "^10.1.4",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "concurrently": "^3.6.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-prettier": "2.6.0",
    "prettier": "1.13.5"
  },
  "dependencies": {
    "@patternson/registry-client": "1.2.8",
    "@stencil/core": "^0.9.11",
    "archiver": "2.1.1",
    "babel-runtime": "6.26.0",
    "chalk": "2.4.1",
    "cli-spinners": "1.3.1",
    "commander": "2.15.1",
    "connect": "^3.6.6",
    "draftlog": "1.0.12",
    "find-free-port": "^1.2.0",
    "graphql-tag": "2.9.2",
    "import-local-file": "0.2.0",
    "node-fetch": "2.1.2",
    "path-match": "^1.2.4",
    "rc": "1.2.8",
    "read-pkg-up": "3.0.0",
    "serve-static": "^1.13.2",
    "stringify-attributes": "^1.0.0",
    "update-notifier": "2.5.0"
  }
}
